(0x7E)iand1: MAR=SP=SP-1; rd
H=TOS
MDR=TOS=MDR AND H; wr; goto Main1
(0x80)ior1:MAR=SP=SP-1; rd
H=TOS
MDR=TOS=MDR OR H; wr; goto Main1
(0x59)dup1: MAR = SP = SP+1

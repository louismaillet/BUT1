PC=OPC+H; fetch
goto Main1
(0x9B)iflt1: MAR=SP=SP-1; rd;
OPC=TOS
TOS=MDR
N=OPC; if(N) goto T; else goto F
(0x99)ifeq1:MAR=SP=SP-1; rd;
OPC=TOS
TOS=MDR
